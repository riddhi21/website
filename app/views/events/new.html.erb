<% if @events.errors.any? %>
<div id="error_explanation">
<h2><%= pluralize(@events.errors.count, "error") %>
prohibited this <%= @events.class.to_s.underscore.humanize.downcase %>
from being saved:</h2>
<p>There were problems with the following fields:</p>
<ul>
<% @events.errors.full_messages.each do |msg| %>
<li><%= msg %></li>
<% end %>
</ul>
</div>
<% end %>
<%= form_for (@events ) do |f| %>
	<%= f.label :Date  %>
	<%= f.text_field :date %>
	<%= f.label :Suburbs %>
	<%= f.text_field :suburb %>
	<%= f.label :Venue %>
	<%= f.text_field :venue%>
	<%= f.label :Time  %>
	<%= f.text_field :time %>
	<%= f.label :Judges%>
	<%= f.text_field :judges%>
	
	 <%= f.submit "Submit", :disable_with => 'Submiting...' %>
	  
<% end %>

